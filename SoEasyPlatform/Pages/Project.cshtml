@page
@{
    ViewData["Title"] = "方案管理";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}
@section js{
    <script src="~/views/system/project.js@(ViewBag.version)"></script>
}
<div class="x_panel search_box">
    <div class="x_title">
        <h2><i class="fa fa-home"></i> @ViewData["Title"]</h2>
        <div class="clearfix"></div>
    </div>

    <div class="x_content main-box">
        <form id="divFrom" class="form-inline">
            <div class="form-group">
                <label for="ex3" class="col-form-label">名称：</label>
                <input type="text" id="ProjentName" name="ProjentName" class="form-control input w-150" placeholder=" ">
            </div>
            <div class="form-group"  >
                <label for="ex3" class="col-form-label">分类：</label>
                <input type="text" id="txtModelIdName" name="ModelIdName" placeholder="Select" class="w-150" autocomplete="off" />
                <input id="txtModelId" type="hidden" value="0" name="ModelId" />
            </div>
            <button id="btnSearch" type="button" class="btn btn-blue m-l-15"><i class="fa fa-search"></i>搜索</button>
            <button id="btnReset" type="button" class="btn btn-info"><i class="fa fa-refresh"></i>重置</button>
            @Html.Partial("Page")
        </form>
    </div>
    <div class="x_content">
        <button id="btnAdd" style="display:none" type="button" class="btn btn-blue"><i class="fa fa-plus"></i>添加</button>
        <button id="btnEdit" style="display:none"  type="button" class="btn btn-blue"><i class="fa fa-edit"></i>修改</button>
        <button id="btnDel" type="button" class="btn btn-search"><i class="fa fa-close"></i>删除</button>
    </div>
    <div class="x_content">
        <table id="divGrid"></table>
        <div id="divGridPage" class="pagination"></div>
    </div>
    <div id="divOpen" style="display:none;">
     
    </div>
</div>
